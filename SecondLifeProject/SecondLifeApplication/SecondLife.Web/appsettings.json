{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=SecondLifeDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Stripe": {
    "PublishableKey": "[PUBLISHABLE_KEY]",
    "SecretKey": "[SECRET_KEY]"
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "SmtpUserName": "[EMAIL CLIENT NAME]",
    "SmtpPassword": "[EMAIL CLIENT PASSWORD]",
    "SmtpServerPort": 587,
    "EnableSsl": true,
    "EmailDisplayName": "SecondLife Application",
    "SendersName": "SecondLife Application"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
